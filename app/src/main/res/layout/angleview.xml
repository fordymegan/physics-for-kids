<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical" >

        <LinearLayout
            android:id="@+id/channelInfor"
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/statusTitler"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Status: "
                android:textAppearance="?android:attr/textAppearanceMedium"
                />
            <TextView
                android:id="@+id/attachedTxtr"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Detached"
                android:textAppearance="?android:attr/textAppearanceMedium"
                />
        </LinearLayout>
        <LinearLayout android:layout_height="wrap_content" android:id="@+id/linearLayout4r" android:weightSum="1" android:layout_width="691dp">
            <TextView
                android:layout_width="wrap_content"
                android:id="@+id/textView3r"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Name: "/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="@string/unknown_val" android:id="@+id/nameTxtr"/>
        </LinearLayout>
        <LinearLayout android:layout_height="wrap_content" android:id="@+id/linearLayout6r" android:layout_width="fill_parent">
            <LinearLayout
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="0dp"
                android:layout_weight="3"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >
                <TextView
                    android:layout_height="wrap_content"
                    android:id="@+id/textView2r"
                    android:text="Serial Number: "
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_width="wrap_content"/>
                <TextView
                    android:layout_height="wrap_content"
                    android:id="@+id/serialTxtr"
                    android:text="@string/unknown_val"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_width="wrap_content"/>
            </LinearLayout>
            <LinearLayout
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >
                <TextView
                    android:layout_height="wrap_content"
                    android:id="@+id/textView4r"
                    android:text="Version: "
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_width="wrap_content"/>
                <TextView
                    android:layout_height="wrap_content"
                    android:id="@+id/versionTxtr"
                    android:text="@string/unknown_val"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_width="wrap_content"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:layout_height="wrap_content" android:id="@+id/linearLayout7r" android:layout_width="fill_parent">
            <LinearLayout
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="0dp"
                android:layout_weight="3"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >
                <TextView
                    android:layout_height="wrap_content"
                    android:id="@+id/textView6r"
                    android:text="Channel: "
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_width="wrap_content"/>
                <TextView android:layout_height="wrap_content"
                    android:id="@+id/channelTxtr"
                    android:text="@string/unknown_val"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_width="wrap_content"/>
            </LinearLayout>
            <LinearLayout
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >
                <TextView
                    android:layout_height="wrap_content"
                    android:id="@+id/textView7r"
                    android:text="Hub Port: "
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_width="wrap_content"/>
                <TextView
                    android:layout_height="wrap_content"
                    android:id="@+id/hubPortTxtr"
                    android:text="@string/unknown_val"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_width="wrap_content"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:weightSum="1" android:id="@+id/linearLayout2r" android:layout_width="597dp" android:layout_height="50dp">
            <TextView
                android:layout_width="wrap_content"
                android:id="@+id/textView5r"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Label: "
                android:layout_height="fill_parent"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="@string/unknown_val"
                android:id="@+id/labelTxtr"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/settingsAndDatar"
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/positionVelocityInfor"
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <LinearLayout
                    android:id="@+id/positionInfor"
                    xmlns:android="http://schemas.android.com/apk/res/android"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/positionTitler"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Position: "
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:visibility="gone"
                        />

                    <TextView
                        android:id="@+id/positionTxtr"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="  "
                        android:visibility="gone"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/velocityInfor"
                    xmlns:android="http://schemas.android.com/apk/res/android"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/velocityTitler"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Velocity: "
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        />

                    <TextView
                        android:id="@+id/velocityTxtr"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="  "
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/engagedSectionr"
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >
                <Button
                    android:id="@+id/engagedButtonr"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="Engage"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:background="#0099cc"
                    android:padding="5dp"
                    />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/whichPlanet"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAllCaps="true"
                    android:text="You have found: "
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    />
                <TextView
                    android:id="@+id/whichPlanetID"
                    android:layout_width="105dp"
                    android:layout_height="match_parent"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    />


            </LinearLayout>

            <LinearLayout
                android:id="@+id/targetPositionSectionr"
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >
                <LinearLayout
                    xmlns:android="http://schemas.android.com/apk/res/android"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">


                    <SeekBar
                        android:id="@+id/targetPositionBarr"
                        android:layout_weight="3.5"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:max="180"
                        />
                </LinearLayout>
                <LinearLayout
                    xmlns:android="http://schemas.android.com/apk/res/android"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/targetPositionLabelr"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Axis of Rotation: "
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        />
                    <TextView
                        android:id="@+id/targetPositionTxtr"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text="Target Position"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/accelerationSectionr"
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <LinearLayout
                    xmlns:android="http://schemas.android.com/apk/res/android"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/accelerationLabelr"
                        android:layout_width="105dp"
                        android:layout_height="wrap_content"
                        android:text="Acceleration: "
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        />

                    <SeekBar
                        android:id="@+id/accelerationBarr"
                        android:layout_weight="3.5"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:max="1000"
                        />
                </LinearLayout>
                <LinearLayout
                    xmlns:android="http://schemas.android.com/apk/res/android"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/accelerationTxtr"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:gravity="right"
                        />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/velocityLimitSectionr"
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <LinearLayout
                    xmlns:android="http://schemas.android.com/apk/res/android"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/velocityLimitLabelr"
                        android:layout_width="105dp"
                        android:layout_height="wrap_content"
                        android:text="Velocity Limit: "
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        />
                    <SeekBar
                        android:id="@+id/velocityLimitBarr"
                        android:layout_weight="3.5"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:max="1000"
                        />
                </LinearLayout>
                <LinearLayout
                    xmlns:android="http://schemas.android.com/apk/res/android"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/velocityLimitTxtr"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:gravity="right"
                        />
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>

    </LinearLayout>
</ScrollView>